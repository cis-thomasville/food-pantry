    "@react-router/serve": "^7.7.1",
    "better-auth": "^1.3.5",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
